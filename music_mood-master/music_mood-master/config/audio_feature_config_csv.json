{
    "agent": "MulticlassClassificationAgent",
    "model": "MulticlassClassification",
    "data_mode": "csv",
    "device": "cpu",

    "mode": "train",
    "id_col": "id",
    "moods": ["calm", "energetic", "happy", "sad"],
    "train_col": ["danceability", "energy", "key", "mode",
         "speechiness", "acousticness","instrumentalness", "liveness",
         "valence","tempo","time_signature"],
    "predict_col": "mood",
    "checkpoint_dir": "checkpoint",
    "checkpoint_file": "MulticlassClassification.pth.tar",
    "seed": 80,
    "train_size_percent": 0.75,
    "validation_size_percent": 0.5,
    "imbalance": true,

    "epochs": 10,
    "batch_size":128,
    "learning_rate":0.01

}
